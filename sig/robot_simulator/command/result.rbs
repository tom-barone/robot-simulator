module RobotSimulator
  module Command
    class Result
      attr_reader error: StandardError?
      attr_reader value: Object?

      def initialize: (?error: StandardError?, ?value: Object?) -> void

      def self.success: (?Object? value) -> Result

      def self.error: (singleton(StandardError) | StandardError err) -> Result

      def success?: () -> bool

      def error?: () -> bool

      def error_message: () -> String?
    end
  end
end
