module RobotSimulator
  module Command
    class Result
      attr_reader error: Error?
      attr_reader value: Object?

      def initialize: (?error: Error?, ?value: Object?) -> void

      def self.success: (?Object? value) -> Result

      def self.error: (singleton(Error) error_class) -> Result

      def success?: () -> bool

      def error?: () -> bool

      def error_message: () -> String?
    end
  end
end
