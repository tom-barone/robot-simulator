module RobotSimulator
  module Command
    class Result
      attr_reader error: Error?
      attr_reader value: String?

      def initialize: (bool success, Error? error, String? value) -> void

      def self.success: (?String? value) -> Result

      def self.error: (singleton(Error) error_class) -> Result

      def success?: () -> bool

      def error?: () -> bool

      def error_message: () -> String?
    end
  end
end
