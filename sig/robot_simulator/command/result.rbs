module RobotSimulator
  module Command
    class Result
      attr_reader error: Error?

      def initialize: (bool success, Error? error) -> void

      def self.success: () -> Result

      def self.error: (singleton(Error) error_class) -> Result

      def success?: () -> bool

      def error?: () -> bool

      def error_message: () -> String?
    end
  end
end
